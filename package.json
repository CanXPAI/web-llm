{
  "name": "@mlc-ai/web-llm",
  "version": "0.2.80",
  "description": "Hardware accelerated language model chats on browsers",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mlc-ai/web-llm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CanXPAI/web-llm.git"
  },
  "keywords": [
    "llm",
    "large language model",
    "machine learning",
    "webgpu",
    "wasm"
  ],

  "type": "module",

  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "browser": "./lib/index.js",
  "types": "./lib/index.d.ts",

  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },

  "files": [
    "lib"
  ],

  "scripts": {
    "build": "rollup -c",
    "lint": "npx eslint ./src/ ./tests/ ./examples/ && npx prettier ./src/ ./tests/ ./examples/ --check",
    "test": "yarn jest",
    "format": "prettier --write \"./src/\" \"./examples/\" \"./tests/\"",
    "prepare": "husky"
  },

  "dependencies": {
    "loglevel": "^1.9.1"
  },

  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.9.0",
    "@mlc-ai/web-runtime": "^0.23.0-dev1",
    "@mlc-ai/web-tokenizers": "^0.1.6",
    "@mlc-ai/web-xgrammar": "0.1.0",
    "@next/eslint-plugin-next": "^14.2.3",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/chrome": "^0.0.266",
    "@types/jest": "^30.0.0",
    "@types/serviceworker": "^0.0.86",
    "@webgpu/types": "^0.1.24",
    "buffer": "^5.7.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.0.11",
    "jest": "^30.2.0",
    "prettier": "3.6.2",
    "process": "^0.11.10",
    "rollup": "^4.12.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.4.5",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.47.0"
  },

  "overrides": {
    "test-exclude": "^7.0.1"
  }
}
